            <kind>1</kind>
            <string>/// Draw

// Click
if(mouse_check_button_pressed(mb_left)) {
    if(mouse_x &gt; room_width/5 * 2)and(mouse_x &lt; room_width/5 * 3)and(mouse_y &lt; 50) {
        matching *= -1;
        time = 0;
        if(matching == 1) {
            var json_data = ds_map_create();
            ds_map_add(json_data, "id", NN.signal_search);
            ds_map_add(json_data, "msg", 1);
            ds_map_add(json_data, "uuid", global.uuid);
            var body = json_encode(json_data);
            ds_map_destroy(json_data);
            nn_send_message(body);
        }else {
            var json_data = ds_map_create();
            ds_map_add(json_data, "id", NN.signal_search);
            ds_map_add(json_data, "msg", 2);
            ds_map_add(json_data, "msg", 1);
            ds_map_add(json_data, "uuid", global.uuid);
            var body = json_encode(json_data);
            ds_map_destroy(json_data);
            nn_send_message(body);
        }
    }
    
    if(mouse_x &gt; 0)and(mouse_x &lt; room_width/5)and(mouse_y &lt; 50) {
        menu = 3;
    }
    
    if(mouse_x &gt; room_width/5)and(mouse_x &lt; room_width/5 * 2)and(mouse_y &lt; 50) {
        menu = 0;
    }
    
    if(mouse_x &gt; room_width/5 * 3)and(mouse_x &lt; room_width/5 * 4)and(mouse_y &lt; 50) {
        menu = 1;
    }
    
    if(mouse_x &gt; room_width/5 * 4)and(mouse_x &lt; room_width/5 * 5)and(mouse_y &lt; 50) {
        menu = 2;
    }
}

// Draw - background
draw_set_color(c_black);
switch(menu) {
    case 0:
        draw_text(room_width/2, room_height/2, "인벤토리");
    break;
    
    case 1:
        draw_text(room_width/2, room_height/2, "상점");
    break;
    
    case 2:
        draw_text(room_width/2, room_height/2, "랭크");
    break;
    
    case 3:
        //draw_text(room_width/2, room_height/2, "홈");
        draw_set_color($cfcfcf);
        draw_roundrect(20, 70, 70, 100, false);
        draw_roundrect(20, 110, 70, 140, false);
        
        draw_set_color(global.iuSky);
        draw_roundrect(room_width/2 - 125, room_height/2 - 95, room_width/2 + 125, room_height/2 + 30, false);
        
        draw_set_color(c_black);
        draw_roundrect(20, 70, 170, 100, true);
        draw_roundrect(20, 110, 170, 140, true);
        draw_roundrect(room_width/2 - 130, room_height/2 - 100, room_width/2 + 130, room_height/2 + 100, true);
        
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_text(45, 85, "레벨");
        draw_text(45, 125, "골드");
        draw_text(45, room_height - 25, "이벤트");
        draw_text(105, room_height - 25, "미션");
        draw_text(room_width/2, room_height/2 + 50, "닉네임");
        
        draw_set_halign(fa_right);
        draw_text(160, 85, "100");
        draw_text(160, 125, "124312415G");
        
        draw_roundrect(20, room_height - 40, 70, room_height - 10, true);
        draw_roundrect(80, room_height - 40, 130, room_height - 10, true);
        
        draw_set_color($cfcfcf);
        draw_set_halign(fa_center);
        draw_text(room_width/2, room_height/2 + 70, "상태 메세지");
    break;
}

// Draw - menu strip
draw_set_color(global.iuSky);
draw_roundrect(0, -1, room_width/5, 50, false);

draw_set_color($cfcfcf);
draw_roundrect(room_width/5, -1, room_width/5 * 2, 50, false);

if(matching == -1)
    draw_set_color(global.iuSky);
else
    draw_set_color(c_orange);
draw_roundrect(room_width/5 * 2, -1, room_width/5 * 3, 50, false);

draw_set_color($cfcfcf);
draw_roundrect(room_width/5 * 3, -1, room_width/5 * 4, 50, false);

draw_set_color(global.iuDark);
draw_roundrect(room_width/5 * 4, -1, room_width/5 * 5, 50, false);

draw_set_halign(fa_center);
draw_set_valign(fa_middle);
draw_set_font(fnt_text);

draw_set_color(c_white);
draw_text(room_width/5/2, 25, "home");
if(matching == -1)
    draw_text(room_width/5*2 + room_width/5/2, 25, "게임 시작");
else {
    time++;
    var time2 = floor(time/60);
    draw_text(room_width/5*2 + room_width/5/2, 25, "매치 메이킹 하는 중#" + string(floor(time2/60)) + " : " + string(time2%60));
}
draw_text(room_width/5*4 + room_width/5/2, 25, "랭크");

draw_set_color(c_black);
draw_text(room_width/5 + room_width/5/2, 25, "인벤토리");
draw_text(room_width/5*3 + room_width/5/2, 25, "상점");

draw_set_color(c_white);
draw_set_halign(fa_left);
draw_set_valign(fa_top);
</string>
